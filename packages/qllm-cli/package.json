{
  "name": "qllm",
  "version": "1.1.1",
  "description": "QLLM Multi-Provider LLM Command CLI",
  "main": "dist/src/cli.js",
  "scripts": {
    "dev-cli": "ts-node -r tsconfig-paths/register src/cli.ts",
    "dev-server": "ts-node -r tsconfig-paths/register src/server.ts",
    "build": "tsc",
    "test": "jest",
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "start": "node dist/src/server.js",
    "cli": "node dist/src/cli.js",
    "prune": "ts-prune prune"
  },
  "keywords": [
    "llm",
    "cli",
    "ai"
  ],
  "bin": {
    "qllm": "dist/qllm.js"
  },
  "baseUrl": ".",
  "paths": {
    "@/*": ["src/*"]
  },
  "author": "quantalogic",
  "license": "Apache-2.0",
  "dependencies": {
    "@anthropic-ai/bedrock-sdk": "^0.10.1",
    "@mistralai/mistralai": "^0.5.0",
    "@qllm/core": "file:../qllm-core",
    "@types/cors": "^2.8.17",
    "@types/helmet": "^4.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.8",
    "@types/prompts": "^2.4.9",
    "axios": "^1.7.2",
    "body-parser": "^1.20.2",
    "cli-spinner": "^0.2.10",
    "commander": "^12.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "fast-xml-parser": "^4.4.0",
    "glob": "^10.4.2",
    "groq-sdk": "^0.5.0",
    "helmet": "^7.1.0",
    "js-yaml": "^4.1.0",
    "lru-cache": "^10.3.0",
    "morgan": "^1.10.0",
    "ollama": "^0.5.2",
    "openai": "^4.52.2",
    "perplexity-sdk": "^1.0.4",
    "prompts": "^2.4.2",
    "winston": "^3.13.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[tj]s?(x)"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "overrides": {
    "glob": "^10.4.2",
    "inflight": "npm:@isaacs/inflight@^1.0.1"
  },
  "devDependencies": {
    "@types/cli-spinner": "^0.2.3",
    "@types/express": "^4.17.21",
    "tsconfig-paths": "^4.2.0"
  }
}

{
  "name": "qllm",
  "version": "1.1.0",
  "description": "QLLM Multi-Provider LLM Command CLI",
  "main": "dist/qllm.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "dev": "ts-node src/qllm.ts",
    "start": "node dist/qllm.js",
    "cli": "node dist/qllm.js",
    "prune": "ts-prune prune"
  },
  "keywords": [
    "llm",
    "cli",
    "ai"
  ],
  "bin": {
    "qllm": "dist/qllm.js"
  },
  "author": "quantalogic",
  "license": "Apache-2.0",
  "dependencies": {
    "@anthropic-ai/bedrock-sdk": "^0.10.1",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.14.8",
    "@types/prompts": "^2.4.9",
    "cli-spinner": "^0.2.10",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "fast-xml-parser": "^4.4.0",
    "glob": "^10.4.2",
    "js-yaml": "^4.1.0",
    "lru-cache": "^10.3.0",
    "ollama": "^0.5.2",
    "openai": "^4.52.2",
    "prompts": "^2.4.2",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@types/cli-spinner": "^0.2.3",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "typescript": "^5.5.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[tj]s?(x)"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "overrides": {
    "glob": "^10.4.2",
    "inflight": "npm:@isaacs/inflight@^1.0.1"
  }
}
